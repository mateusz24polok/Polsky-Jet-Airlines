{
  "name": "polsky-jet-page",
  "version": "0.0.1",
  "description": "Project of fake airlines Polsky Jet page with system of tickets reservation",
  "main": "server/server.js",
  "scripts": {
    "server:clear": "rimraf dist",
    "server:build": "npm run server:clear && tsc",
    "server:start": "npm run server:build && node server/dist/server.js",
    "server:dev": "nodemon --exec ts-node server/src/server.ts --watch src",
    "postinstall": "npm run server:build"
  },
  "keywords": [
    "airlines",
    "travel",
    "polsky",
    "jet",
    "hotels",
    "cars",
    "flight",
    "ticket"
  ],
  "author": "Mateusz Polok",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.41",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1"
  }
}
